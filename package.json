{
  "name": "nuxt-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build  --dotenv .env",
    "dev": "nuxt dev --dotenv .env.local",
    "start:static": "nuxt start --dotenv .env",
    "start": "nuxt build && node .output/server/index.mjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest run --coverage --dangerouslyIgnoreUnhandledErrors",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "cypress:open": "cypress open --e2e --browser edge",
    "cypress:build-open": "npm run generate && npm run start:static",
    "cypress:run": "cypress run --browser edge",
    "cypress:runFF": "cypress run --browser firefox"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@nuxt/devtools": "^1.3.3",
    "@nuxt/test-utils": "^3.13.1",
    "@pinia/nuxt": "^0.5.1",
    "@pinia/testing": "^0.1.3",
    "@testing-library/cypress": "^10.0.2",
    "@types/node": "^20.14.2",
    "@types/papaparse": "^5.3.14",
    "@vitest/browser": "^1.2.2",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "@vue/test-utils": "^2.4.4",
    "@vueuse/core": "^10.11.0",
    "date-fns": "^3.6.0",
    "happy-dom": "^14.12.3",
    "nuxt": "^3.12.2",
    "ofetch": "1.3.4",
    "playwright-core": "^1.45.0",
    "prettier": "^3.2.5",
    "typescript": "5.4.5",
    "vite-plugin-vuetify": "^2.0.1",
    "vitest": "^1.2.2",
    "vue": "^3.4.30",
    "vue-router": "^4.4.0",
    "vue-tsc": "^1.8.27",
    "vuetify": "^3.6.10"
  }
}
